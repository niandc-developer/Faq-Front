<!DOCTYPE html>
<html lang="ja">
<head>
<%- include('./common/commonHead') %>
<!-- setting panel size -->
<link href="../stylesheets/panelSize.css" rel="stylesheet" type="text/css">
<!-- security setting -->
<meta name="_csrf" content="<%= csrfToken %>">
</head>
<body>
  <div id="wrapper">
    <%- include('./common/commonNavi') %>
    <div id="page-wrapper">
      <div class="container-fluid">
        <div class="row">
          <!-- chat -->
          <div class="col-lg-12">
            <h3></h3>
          </div>
          <div class="col-lg-12">
              <!-- panel -->
              <div class="chat-panel panel panel-default">
                <!-- input text form -->
                <div class="panel-heading">
                  <div class="input-group">
                    <span class="input-group-btn">
                      <button class="btn btn-info btn-sm" id="clickChatBtn">
                        Send
                      </button>
                    </span>
                    <input id="inputTextForm" type="text" class="form-control input-sm"
                      placeholder="質問を入力して下さい"/>
                  </div>
                </div>
                <!-- RaR answer display -->
                <div class="panel-body">
                  <ul class="chat">
                  </ul>
                </div>
              <!-- /panel -->
              </div>
          </div>
        <!-- /chat -->
        </div>
      </div>
    </div>
  </div>
  <%- include('./common/commonScripts') %>
  <%- include('./common/commonScriptsFaq') %>
</body>
</html>